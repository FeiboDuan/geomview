  HAVE_XMU=1
  if test "$GEOM_L_OPTION" != "" ; then
    XMULIBS="$GEOM_L_OPTION -lXmu"
  else 
    XMULIBS="-lXmu"
  fi
else
  HAVE_XMU=0
fi

if test "$HAVE_XMU" = "1" ; then
  AC_DEFINE(HAVE_XMU)
fi
AC_SUBST(XMULIBS)

