AC_ARG_WITH(forms,
	    [--with-forms=DIR      set DIR to forms installation dir],
	    [ if test "$withval" = "no" ; then
                TRY_FORMS=false
              else
                TRY_FORMS=true
                FORMS_DIR=$withval
              fi ;
            ],
            [ TRY_FORMS=true ]
           )

if $TRY_FORMS ; then
    if test "$FORMS_DIR" != "" ; then
      USER_FORMS_DIR="$FORMS_DIR"
    else
      # if we're on a Linux iX86 system, guess the libc version and use the
      # corresponding version of forms that we distribute in the xforms
      # subdir
      case "$target" in
       *86*linux*) 
          GEOM_FIND_LIBC_VERSION
          if test "$GEOM_LIBC_VERSION" = "5" ; then
            USER_FORMS_DIR="`pwd`/xforms/bxform-089/FORMS"
          else
            USER_FORMS_DIR="`pwd`/xforms/bxform-089-glibc2.1/FORMS"
          fi
          ;;
       *)
          USER_FORMS_DIR=""
          ;;
      esac
    fi

    GEOM_FIND_I_OPTION(forms.h, ["$USER_FORMS_DIR" "" "/usr/local/include"])
    FORMSINCLUDE=$GEOM_I_OPTION
    AC_SUBST(FORMSINCLUDE)
    
    GEOM_FIND_L_OPTION(-lforms,
    		   fl_initialize,
    		   ["$USER_FORMS_DIR" "" "/usr/local/lib"],
    		   "$XLIBS -lm"
    		  )
    if test "$GEOM_L_OPTION" != "0" ; then
      FORMS_L_OPTION=$GEOM_L_OPTION
      FORMSLIBS="$FORMS_L_OPTION -lforms"
      HAVE_FORMS=true 
      FORMS_LIB_SUBDIRS="forms"
    else
      FORMSLIBS=""
      FORMS_LIB_SUBDIRS=""
    fi

else
  FORMSLIBS=""
  FORMS_LIB_SUBDIRS=""
fi

if test "$FORMSLIBS" = "" ; then
  AC_MSG_WARN([

    Configuring without the FORMS library.  Several of Geomview's
    external modules use the FORMS library, and they will not be
    compiled unless you re-run configure with the argument
    --with-forms=DIR where DIR is the pathname of the directory
    containing both the FORMS header file forms.h, and the FORMS
    library files (libforms.a, libforms.so, etc).  Geomview itself
    does not use FORMS, so this is fine if all you want is
    Geomview.

])
fi

AC_SUBST(FORMSLIBS)
AC_SUBST(FORMS_LIB_SUBDIRS)

