########################################################################

#
# AC_PATH_X sets $x_libraries and $x_includes
#
AC_PATH_X

#
# If $x_includes is nonempty, remember it in XINCLUDES
#
if test "$x_includes" != "" ; then
  XINCLUDES=-I$x_includes
else
  XINCLUDES=
fi
AC_SUBST(XINCLUDES)

#
# Find out how to link with the various X libraries we need
#
GEOM_FIND_L_OPTION(-lX11,
		   XOpenDisplay,
		   ["" "$x_libraries" "$MOTIF_DIR/lib"])
X11_L_OPTION=$GEOM_L_OPTION
GEOM_FIND_L_OPTION(-lXt,
		   XtCreateManagedWidget,
		   ["" "$x_libraries" "$MOTIF_DIR/lib"])
if test "$GEOM_L_OPTION" != "$X11_L_OPTION" ; then
  Xt_L_OPTION=$GEOM_L_OPTION
else
  Xt_L_OPTION=""
fi
GEOM_FIND_L_OPTION(-lXext,
		   XextCreateExtension,
		   ["" "$x_libraries" "$MOTIF_DIR/lib"])
if test "$GEOM_L_OPTION" != "$X11_L_OPTION"  \
     -a "$GEOM_L_OPTION" != "$Xt_L_OPTION"  ; then
  Xext_L_OPTION=$GEOM_L_OPTION
else
  Xext_L_OPTION=""
fi

# Store the whole shebang in $XLIBS:
XLIBS="$X11_L_OPTION -lX11 $Xt_L_OPTION -lXt $Xext_L_OPTION -lXext"
AC_SUBST(XLIBS)

########################################################################

GEOM_FIND_L_OPTION(-lXmu,
		   XmuLookupStandardColormap,
                   ["" "$x_libraries" "$MOTIF_DIR/lib"],
		   $XLIBS)
